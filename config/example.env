
# Your Discord token (required)
DISCORD_TOKEN=your_discord_token_here

# Additional tokens for multi-bot support (optional)
DISCORD_TOKEN_2=your_second_discord_token_here
DISCORD_TOKEN_3=your_third_discord_token_here
# Add more tokens as needed: DISCORD_TOKEN_4, DISCORD_TOKEN_5, etc.

# Groq API key (get from https://console.groq.com/keys)
GROQ_API_KEY=your_groq_api_key_here

# OpenAI API key (optional - get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=your_openai_api_key_here

# Webhook URL for error notifications (optional)
WEBHOOK_URL=your_webhook_url_here
